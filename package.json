{
  "name": "@mayple/hubspot-form-submit-v3",
  "version": "1.0.8",
  "description": "SDK library for Mayple websites",
  "main": "build/hubspot-form-submit-v3.js",
  "types": "build/types/index.d.ts",
  "scripts": {
    "build": "webpack && tsc",
    "test": "jest --silent",
    "coverage": "yarn run test --silent --coverage",
    "prepare": "yarn run build",
    "trypublish": "npm publish --access=public || true",
    "prettier-fix": "prettier --write \"src/**/*.{js,ts,jsx,tsx,json}\"",
    "precommit": "./precommit.sh"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:eyalgursoy/hubspot-form-submit-v3.git"
  },
  "author": "Eyal Gursoy<eyal.gursoy@mayple.com>",
  "license": "MIT",
  "keywords": [
    "Hubspot",
    "form",
    "submit",
    "browser"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn run precommit"
    }
  },
  "dependencies": {
    "cross-fetch": "^3.1.5",
    "isomorphic-fetch": "^3.0.0",
    "js-cookie": "^3.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-modules-commonjs": "^7.19.6",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/plugin-transform-typescript": "^7.20.2",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.20.1",
    "@types/express": "^4.17.13",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/jest": "^27.4.0",
    "@types/js-cookie": "^2.2.7",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "babel-preset-minify": "^0.5.0",
    "clean-webpack-plugin": "^4.0.0",
    "core-js": "3",
    "css-loader": "^6.5.1",
    "eslint": "^7.32.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "4.3.8",
    "jest": "^29.2.2",
    "prettier": "^2.7.1",
    "prettier-webpack-plugin": "^1.2.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.0",
    "typescript": "^4.8.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "\\.[jt]sx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|graphql)$": "<rootDir>/scripts/testMock.js",
      "\\.(css|less)$": "<rootDir>/scripts/testMock.js"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
